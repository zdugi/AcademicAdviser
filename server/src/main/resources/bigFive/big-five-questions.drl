package bigFive;
import com.academic.adviser.drools.model.BigFiveAnswer;
import com.academic.adviser.model.BigFiveResults;

rule "BigFive Question_50"
agenda-group "big-five-questions"
lock-on-active true
when
    $p: BigFiveAnswer(id == 50)
    $r: BigFiveResults()
then
    modify ($r) { addOpenness($p.getScore()) };
end

